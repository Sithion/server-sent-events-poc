{
  "name": "microservice",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "cross-env NODE_ENV=development nest start | pino-pretty",
    "dev": "cross-env NODE_ENV=development nest start --watch | pino-pretty",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch | pino-pretty",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=development jest",
    "test:ci": "cross-env NODE_ENV=development jest --watchAll=false",
    "test:watch": "cross-env NODE_ENV=development jest --watch",
    "test:cov": "cross-env NODE_ENV=development jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/cosmos": "^4.1.0",
    "@azure/msal-node": "^2.12.0",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/swagger": "^7.1.10",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "http-status-codes": "^2.3.0",
    "pino-pretty": "^11.2.2"
  },
  "devDependencies": {
    "packages.config": "workspace:*",
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.3.9",
    "@playwright/test": "1.40.1",
    "@types/express": "^4.17.13",
    "@types/node": "22.5.0",
    "@types/passport-azure-ad": "^4.3.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "node-mocks-http": "^1.14.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "cookie": "^0.7.0",
    "express": "^4.20.0",
    "body-parser": "^1.20.3"
  }
}